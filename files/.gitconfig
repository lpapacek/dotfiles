  email   = papacseklehel@gmail.com

[alias] 
  a       = add
  st      = status
  cm      = commit -m
  br      = branch
  cho     = checkout
  pl      = pull
  ps      = push
  l       = log
  ll      = !git --no-pager log --pretty=format:'%C(yellow)%h%Creset %Cgreen(%><(12)%cr%><|(12))%Creset - %s %C(blue)<%an>%Creset' -10

[color]
  diff    = auto
  status  = auto
  branch  = auto

[core]
  editor  = vi